    <header role="banner" id="global-header" class="with-proposition">
      <div class="header-wrapper">
        <div class="header-global">
          <div class="header-logo">
            <a href="https://www.gov.uk/" title="Go to the gov.uk homepage" id="logo" class="content">
              <img src="https://assets.digital.cabinet-office.gov.uk/static/gov.uk_logotype-2x-24145374d271079f08d96c618cc8d4a5.png" alt="GOV.UK">
            </a>
          </div>
        </div>
        <div class="header-proposition">
          <div class="content">
            <a href="#proposition-links" class="js-header-toggle menu">Menu</a>
            <nav id="proposition-menu"><a href="/" id="proposition-name">Transition</a>
            <ul id="proposition-links">
              <li><a href="/">Organisations</a></li>
              <% if @site %>
                <li><%= nav_link @site.organisation.title, organisation_path(@site.organisation) %></li>
              <% end %>
              <li class="clear-child"><%= link_to "Signed in as #{current_user.name}", Plek.current.find('signon') %></li>
              <li><%= link_to 'Sign out', gds_sign_out_path %></li>
            </ul></nav>
          </div>
        </div>
      </div>
      </header>
    <!--end header-->

    <div id="global-cookie-message">
      <p>GOV.UK uses cookies to make the site simpler. <a href="https://www.gov.uk/support/cookies">Find out more about cookies</a></p>
